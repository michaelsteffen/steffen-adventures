---
---

{"type":"FeatureCollection",
  "features":
  [
  {% for post in site.posts %}
  {
    "type":"Feature",
    "properties": {
      "id": "post-{{ post.id }}",
      "type": "check-in",
      "date": "{{ post.date | date: "%B %-d, %Y" }}",
      "url": "{{ post.url }}"
    },
    "geometry": {
      "type": "Point",
      "coordinates": [{{ post.lon }}, {{ post.lat }}]
    }
  } {% unless forloop.last %},{% endunless %}
  {% endfor %}
  ]
}
